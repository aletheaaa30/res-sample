@model WebApplication6.Models.user;
@{
    <form asp-action="Home" method="post">

        LoginId:
        @Html.TextAreaFor(u => u.email);
        Password:
        @Html.TextAreaFor(u => u.password);
        <input type="submit" value="Submit" />

    </form>
    <p style="color: red"> @ViewBag.errorMessage</p>

}
